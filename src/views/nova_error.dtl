    <div id='stars3'></div>
    {% if logo %}
    <img src="{{ logo }}" id="logo" />
    {% endif %}
    <div id='title'>
      <span>
        <h1>{{ title }}</h1>
        {{ message }}
        {% if extra_msg %}
        <pre>
          {{ extra_msg|safe }}
          {% if stacktrace %}
          <code class="erlang">
            {% for s in stacktrace %}
            {{ s.module }}:{{ s.function }}/{{ s.arity }} ({{ s.file }}:{{ s.line }})
            {% endfor %}
          </code>
          {% endif %}
        </pre>
        {% endif %}
      </span>
    </div>
    <script>hljs.highlightAll();</script>
